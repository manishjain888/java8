/*
Module	:	Funding AutoFund Messages.
Author	:	Neeraj Joshi	
Date	:	06 August 2008
Purpose	:	This is a sequence to generate the Message ID's.
*/
DROP SEQUENCE rbsgbl_funding_msg_seq
;

CREATE SEQUENCE rbsgbl_funding_msg_seq
INCREMENT BY 1
START WITH 1
MAXVALUE 9999999999999999999999999
MINVALUE 1
CYCLE
NOCACHE
ORDER
;